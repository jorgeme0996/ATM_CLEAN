<% include ../partials/header %>

<h1>Registro de Usuarios</h1>

<form action="/register" method="POST">

    <label>Nombre</label>
    <input type="text" name="data[firstName]">

    <label>Apellido Paterno</label>
    <input type="text" name="data[lastName]">

    <label>Apellido Materno</label>
    <input type="text" name="data[surName]">

    <label>Genero</label>
    <input type="text" name="data[gennder]">

    <label>Pa√≠s</label>
    <input type="text" name="data[birthData.country]">

    <label>Estado</label>
    <input type="text" name="data[birthData.state]">

    <label>Fecha de nacimiento</label>
    <input type="date" name="data[birthData.birthDate]">

    <label>Tipo de documento</label>
    <input type="text" name="data[identityDocuments.documentType]">

    <label>Numero</label>
